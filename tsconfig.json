  "compilerOptions": {
    "target": "es5",
    "jsx": "preserve",                             
    "module": "commonjs",
    "allowJs": true,                               
    "sourceMap": true,                             
    "noEmit": true,                                
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}